{
  "name": "sveltekit-adapter-deno",
  "version": "0.14.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbushell/sveltekit-adapter-deno"
  },
  "description": "Adapter for SvelteKit apps that generates a standalone Deno or Deno Deploy server",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "deno compile --target aarch64-unknown-linux-gnu --allow-read --allow-net --allow-env test/ssl-test.ts && scp -6 ./ssl-test 'root@[2a01:4f8:c2c:780f::1]:/root/app/ssl-test'"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "files",
    "index.js",
    "index.d.ts",
    "package.json"
  ],
  "dependencies": {
    "esbuild": "^0.19.11"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.0.6"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  }
}